{
  "name": "visutry",
  "version": "0.2.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:skip-prisma": "next dev",
    "dev:test": "next dev --port 3001",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "test:mode": "cp .env.test .env.local && npm run dev",
    "test:integration": "node scripts/test-integration.js",
    "test:start": "bash scripts/start-test-mode.sh",
    "test:start:windows": "scripts/start-test-mode.bat",
    "test": "node tests/scripts/run-all-tests.js",
    "test:all": "node tests/scripts/run-all-tests.js all",
    "test:unit": "jest tests/unit --testTimeout=30000",
    "test:integration:new": "jest tests/integration --testTimeout=60000",
    "test:api": "jest tests/integration/api --testTimeout=60000",
    "debug:oauth": "node scripts/debug-twitter-oauth.js",
    "debug:logs": "echo 'Visit http://localhost:3000/debug/logs to view logs'",
    "test:e2e": "jest tests/integration/e2e --testTimeout=120000",
    "test:workflows": "jest tests/integration/workflows --testTimeout=90000",
    "test:watch": "jest --watch tests/",
    "test:coverage": "jest --coverage tests/",
    "test:setup": "node tests/scripts/setup-test-env.js",
    "test:cleanup": "node tests/scripts/cleanup.js",
    "test:e2e:playwright": "playwright test",
    "test:e2e:install": "playwright install",
    "release": "node scripts/release.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "release:patch:skip-tests": "node scripts/release.js patch --skip-tests",
    "release:minor:skip-tests": "node scripts/release.js minor --skip-tests",
    "release:major:skip-tests": "node scripts/release.js major --skip-tests",
    "release:patch:quick": "node scripts/release.js patch --skip-tests --skip-build",
    "release:minor:quick": "node scripts/release.js minor --skip-tests --skip-build",
    "release:major:quick": "node scripts/release.js major --skip-tests --skip-build",
    "version:check": "echo $npm_package_version",
    "version:bump": "npm version --no-git-tag-version",
    "sitemap": "next-sitemap",
    "seo:check": "echo 'Checking SEO configuration...' && node -e \"console.log('Site URL:', process.env.NEXT_PUBLIC_SITE_URL || 'Not set')\""
  },
  "dependencies": {
    "@auth0/nextjs-auth0": "^4.11.0",
    "@google/generative-ai": "^0.24.1",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/adapter-neon": "^6.17.1",
    "@prisma/client": "^6.17.1",
    "@vercel/blob": "^1.1.1",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "https-proxy-agent": "^7.0.6",
    "lucide-react": "^0.294.0",
    "next": "^14.0.0",
    "next-auth": "^4.24.11",
    "next-seo": "^6.5.0",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.4.32",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stripe": "^14.25.0",
    "tailwind-merge": "^2.1.0",
    "tailwindcss": "^3.3.6",
    "use-debounce": "^10.0.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.55.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/ws": "^8.18.1",
    "axios": "^1.12.2",
    "canvas": "^3.2.0",
    "dotenv": "^17.2.2",
    "eslint": "^8.55.0",
    "eslint-config-next": "^14.0.0",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "playwright": "^1.55.0",
    "prisma": "^6.17.1",
    "tsx": "^4.20.5",
    "typescript": "^5.3.3"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  }
}
